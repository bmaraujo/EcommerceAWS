{
  "compilerOptions": {
    "target": "ES2022",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "lib": [
      "es2022"
    ],
    "declaration": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": false,
    "inlineSourceMap": true,
    "inlineSources": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false,
    "skipLibCheck": true,
    "typeRoots": [
      "./node_modules/@types"
    ],
    "baseUrl": "./",
    "paths":{
      "/opt/nodejs/productsLayer":["lamnda/products/layers/productsLayer/nodejs/productRepository"],
      "/opt/nodejs/productEventssLayer":["lamnda/products/layers/productEventssLayer/nodejs/productEvent"],
      "/opt/nodejs/ordersLayer":["lamnda/orders/layers/ordersLayer/nodejs/orderRepository"]
    }
  },
  "exclude": [
    "node_modules",
    "cdk.out"
  ]
}
